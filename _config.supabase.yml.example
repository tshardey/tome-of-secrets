# Optional Supabase config override (DO NOT COMMIT real credentials)
#
# Copy this file to `_config.supabase.yml` (which is git-ignored) and fill in values.
# Then run Jekyll with:
#   bundle exec jekyll serve --config _config.yml,_config.supabase.yml
#
supabase_url: "https://YOUR_PROJECT_REF.supabase.co"
supabase_publishable_key: "sb_publishable_..."

# Optional: serve images from a CDN base (e.g., Supabase Storage public bucket)
# Example:
#   https://YOUR_PROJECT_REF.supabase.co/storage/v1/object/public/tome-of-secrets-images
images_cdn_base: ""


